{
  "name": "dessert-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build && cp -r ./public ./.next/standalone/public && cp -r ./.next/static ./.next/standalone/.next/static",
    "start": "node .next/standalone/server.js",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install",
    "postinstall": "husky install",
    "check-types": "tsc --noemit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:no-watch": "vitest run",
    "coverage": "vitest run --coverage",
    "chromatic": "bash ./scripts/chromatic_publish.sh"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@next/third-parties": "15.3.2",
    "@tanstack/react-query": "^5.8.4",
    "@tanstack/react-query-devtools": "^5.25.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "eslint-config-prettier": "^9.1.0",
    "framer-motion": "^11.1.7",
    "jotai": "^2.12.4",
    "next": "15.3.2",
    "qs": "^6.12.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.51.5",
    "react-icons": "^5.0.1",
    "react-intersection-observer": "^9.8.1",
    "react-markdown": "^9.0.1",
    "swiper": "^11.0.5",
    "tailwind-merge": "^2.2.1",
    "tailwind-scrollbar-hide": "^1.1.7"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.4.0",
    "@storybook/addon-essentials": "^8.1.3",
    "@storybook/addon-interactions": "^8.1.3",
    "@storybook/addon-links": "^8.1.3",
    "@storybook/addon-mdx-gfm": "^8.1.3",
    "@storybook/addon-onboarding": "^8.1.3",
    "@storybook/blocks": "^8.1.3",
    "@storybook/nextjs": "^8.1.3",
    "@storybook/react": "^8.1.3",
    "@storybook/test": "^8.1.3",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4.1.8",
    "@tanstack/eslint-plugin-query": "^5.8.4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20",
    "@types/qs": "^6",
    "@types/react": "19.1.4",
    "@types/react-dom": "19.1.5",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^3.1.4",
    "@vitest/ui": "3.1.4",
    "chromatic": "^11.3.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-next": "15.3.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^8.0.0",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.1.0",
    "postcss": "^8.4.38",
    "postcss-syntax": "^0.36.2",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "storybook": "^8.1.3",
    "tailwindcss": "^4.1.8",
    "typescript": "^5",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "3.1.4"
  },
  "packageManager": "yarn@4.2.2",
  "resolutions": {
    "@types/react": "19.1.4",
    "@types/react-dom": "19.1.5"
  }
}
